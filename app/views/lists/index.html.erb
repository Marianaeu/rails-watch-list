<div class="banner banner_lists">
  <h1>Save any kind of movies</h1>
</div>
<div class="container">





<div class="card-group">
  <% @lists.each do |list| %>
    <div class="card text-bg-dark">
      <img src="<%= list.image_url %>" class="card-img" alt="<%=list.name%>">
      <div class="card-img-overlay">
      <h5 class="card-title"><%= link_to list.name, list_path(list) %></h5>
      </div>
    </div>
  <% end %>
</div>
</div>
